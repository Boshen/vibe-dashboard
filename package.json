{
  "name": "vibe-dashboard",
  "version": "0.1.0",
  "description": "A frontend dashboard for displaying different metrics using bar charts",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter dashboard dev",
    "build": "pnpm -r build",
    "lint": "vp lint --type-aware",
    "test": "pnpm -r test",
    "clean": "pnpm -r clean",
    "build-deploy": "pnpm lint && pnpm build",
    "rolldown:list": "node tool/override-rolldown.mjs --list",
    "rolldown:use": "node tool/override-rolldown.mjs",
    "rolldown:stats": "node tool/override-rolldown.mjs --stats",
    "fmt": "dprint fmt",
    "vp": "vp"
  },
  "keywords": [
    "dashboard",
    "charts",
    "metrics",
    "frontend"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@voidzero-dev/global": "0.0.0-ec375d576832bc5c7ec2ab60473dce4b9211ba7c"
  },
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "dprint": "^0.50.1"
  }
}
